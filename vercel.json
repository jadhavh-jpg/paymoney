{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^/builder.html$", "dest": "/public/builder.html" },
    { "src": "^/p/(.*)$", "dest": "/index.js" },
    { "src": "^/api/(.*)$", "dest": "/index.js" },
    { "src": "^/(.*)$", "dest": "/public/$1" }
  ]
}
